{
    "name": "thecrazybob/laravel-whmcs",
    "description": "A simple Laravel interface for interacting with the WHMCS API.",
    "keywords": ["laravel", "whmcs", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Sohail"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "WHMCS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WHMCS\\WHMCSServiceProvider"
            ],
            "aliases": {
                "WHMCS": "WHMCS\\Facades\\WHMCS"
            }
        }
    },
    "minimum-stability": "dev"
}
